+
+function wa(number, text){
+  const msg = encodeURIComponent(text);
+  window.open(`https://wa.me/${number}?text=${msg}`, '_blank');
+}
+function reservar(property){
+  const entrada = document.querySelector('#entrada')?.value || '';
+  const salida = document.querySelector('#salida')?.value || '';
+  const huespedes = document.querySelector('#huespedes')?.value || '';
+  const nombre = document.querySelector('#nombre')?.value || '';
+  const notas = document.querySelector('#notas')?.value || '';
+
+  const parts = [];
+  parts.push(`¡Hola! Me gustaría reservar *${property}*`);
+  if(entrada) parts.push(`Entrada: ${entrada}`);
+  if(salida) parts.push(`Salida: ${salida}`);
+  if(huespedes) parts.push(`Huéspedes: ${huespedes}`);
+  if(nombre) parts.push(`Nombre: ${nombre}`);
+  if(notas) parts.push(`Notas: ${notas}`);
+
+  const msg = encodeURIComponent(parts.join('\n'));
+  const phone = window.WHATSAPP_PHONE || '34600000000';
+  window.open(`https://wa.me/${phone}?text=${msg}`, '_blank');
+}
+function contacto(){
+  const nombre = document.querySelector('#nombre')?.value || '';
+  const mensaje = document.querySelector('#mensaje')?.value || '';
+  const phone = window.WHATSAPP_PHONE || '34600000000';
+  wa(phone, `Hola, soy ${nombre}. ${mensaje}`);
+}
